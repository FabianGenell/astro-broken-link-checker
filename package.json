{
  "name": "astro-seo-checker",
  "version": "1.0.0",
  "description": "An Astro integration to check for SEO issues and broken links in your website during build time.",
  "main": "dist/index.js",
  "type": "module",
  "exports": {
    ".": "./dist/index.js"
  },
  "types": "dist/index.d.ts",
  "keywords": [
    "astro",
    "astro-integration",
    "broken-links",
    "link-checker",
    "seo",
    "seo-checker",
    "seo-analyzer",
    "accessibility"
  ],
  "author": "Fabian Genell, based on work by Lilith River",
  "license": "Apache License 2.0",
  "dependencies": {
    "fast-glob": "^3.3.2",
    "node-fetch": "^3.3.2",
    "node-html-parser": "^6.1.13",
    "p-limit": "^6.1.0"
  },
  "devDependencies": {
    "@types/node": "^22.15.17",
    "execa": "^9.4.0",
    "typescript": "^5.8.3",
    "vitest": "^2.1.3"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "prepare": "npm run build",
    "test": "cd tests && npm run build && cd .. && vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "typecheck": "tsc --noEmit"
  }
}
